{
  "manifest_version": 2,

  "name": "Dcard Disabler",
  "description": "This extension disables dcard button",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://www.dcard.tw/*"],
      "js": ["disable.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
} 
